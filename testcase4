public class Main {

	public static void main(String[] args) {
		int n=14;
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				if(i==n-1 || i==n-2 || j==0 && i!=5 && i!=4 && i!=3 && i!=2 && i!=1 && i!=0 || j==n-1 && i!=0 && i!=1 && i!=2 && i!=3 && i!=4 && i!=5 || i-j>=(n-1)/2 || i+j>=(n-1) + n/2-1)
					System.out.print("*");
				else
					System.out.print(" ");
			}
			System.out.println();
			}
		}

	}
